num = 14
num2 = num + 1
cen = 1
spp = ''
for steps in range(num):
    while num != cen:
        print(num, end = ' ')
        num -= 1
    print(end = spp)
    num += 1
    while num != num2:
        print(num, end = ' ')
        num += 1
    num -= 1
    print ("")
    cen += 1
    if steps < 8:
        spp = spp + (' ' * 4)
    else:
        spp = spp + (' ' * 6)
